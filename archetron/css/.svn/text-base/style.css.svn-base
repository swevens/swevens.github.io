html, body {width: 100%; height: 100%; margin: 0;padding: 0;overflow: hidden;}
body{
	font-size: 100%;
	color: #fff;
}


.footer{
	font-family: "SapientSansLight";
	
	margin-bottom: 0;
	margin-right: 4em;
	margin-top: 0;
	position: fixed;
	bottom: 10px;
	right: 20px;
}

.close{
	display: none;
	position: absolute;
	right: 20px;
	top:10px;
	width:36px;
	height: 36px;
	background: url('../img/close.png');
	background-repeat: no-repeat;
}

.show-close{
	display: inline-block;
}


#countdown, #info{
	display: table;
	text-align: center;
}

#countdown{
	width: 90%;
}

#info{
	width: 100%;
}

#skillDetail,#empDetail{
	position: absolute;
	font-family: "Orator Std";
	color: #fff;
	border-right: 1px solid #fff;
	border-bottom: 1px solid #fff;
	left:120px;
}

#empDetail{
	top:80px;
}

#skillDetail{
	top:250px;
}

#empDetail h3{
	padding-left: 5px;
	margin-bottom: 0;
	font-size:medium;
	text-align: right;
}

#skillList li,#detail li{
	margin-top: 15px;
	text-align: right;
}

.right-container{
	float: right;
	vertical-align: bottom;
	margin-right: 5em;
}

#info{
	margin:0 auto;

}

#headCount{
	text-shadow: 0 0 5px #fff;
	font-size: 3em;
}

.countdown-block{
	display: table-cell;
	font-family: Georgia;
	font-size: 1.5em;
	background: url('../img/block.png');
	background-repeat: no-repeat;
	width: 82px;
	height: 82px;
	vertical-align: bottom;
}

.spacer{
	border-left: 5px solid transparent;
}

.main-heading, .status{
	font-weight: bold;
}

.main-heading{
	font-family: Georgia;
	font-size: 1.2em;
	text-align: center;
}

.status{
	float: left;
	margin: 0;
	width: 25%;
	font-family: Georgia;
	font-size: 1.9em;
	margin-left: 7em;
}

.cell{
	display: table-cell;
	min-height: 100px;
	min-width: 100px;
	vertical-align: middle;
	opacity: 0.8;
	/*border:1px solid #fff;*/
}

.cell span{
	font-family: Georgia;
}

.base{
	font-family: Georgia;
	font-size: 15px;
	width: 50%;
	margin:0 auto;
}

.wrapper{
	min-height: 350px;
	/*margin-top: 28px;*/
}

.sape-logo{
	position: fixed;
	top:90px;
	/*left:280px;*/
}

.translate-element1{
	transform: translateX(180px);
	-ms-transform: translateX(180px); /* IE 9 */
	-webkit-transform: translateX(180px); /* Safari and Chrome */
	transition: transform 0.5s ease-in-out;
	-webkit-transition: -webkit-transform 0.5s ease-in-out;
}

.translate-element2{
	transform: translateX(0px);
	-ms-transform: translateX(0px); /* IE 9 */
	-webkit-transform: translateX(0px); /* Safari and Chrome */
	transition: transform 0.5s ease-in-out;
	-webkit-transition: -webkit-transform 0.5s ease-in-out;
}

.bg{
	background: url('../img/background/4.jpg');
}

.bg1{
	background: url('../img/background/1.jpg');
	
}

.bg2{
	background: url('../img/background/2.jpg');
}

.bg3{
	background: url('../img/background/3.jpg');
}

.bg,.bg1,.bg2,.bg3{
	background-repeat: no-repeat;
	background-size: cover;
}

.active-projects{
	position: fixed;
	top:15px;
	right: 35px;
	text-align: center;
}

.toggle-menu{
	display: inline-block;
	background: url('../img/menu-open.png');
	width:46px;
	height: 49px;
	top:3px;
	position: absolute;
}

.toggle-menu-collapse{
	background: url('../img/menu-close.png');
}

ul{
	list-style: none;
	padding: 0;
	margin: 0;
}

ul li{
	margin-left: 5px;
	margin-right: 5px;
}

.list{
	background-color: #000;
	width: 180px;
	height: 100%;
	border-right: 1px solid #fff;
	position: absolute;
	top: 0;
	left: -180px
}

.list p{
	font-family: Georgia;
	font-size: 12px;
	background: url('../img/blackGrad.png');
	background-repeat: repeat-x;
	text-align: center;
	border-bottom: 1px dotted #fff;
	height: 30px;
	padding-top: 8px;
}

.list ul li{
	font-family: "SapientSansLight";
	font-size: 12px;
	padding-top: 3px;
	border-bottom: 1px solid #323232;
	height:20px;
}

.list ul li span{
	display: inline-block;
	border-radius:50%;
	width: 8px;
	height: 8px;
	/*background-color: #FFAC0A;*/
	margin-right: 5px;
}

.list ul li a{
	color: #fff;
	text-decoration: none;
}

.left{
	background: url('../img/rect-corner.png');
	width: 20px;
	height: 44px;
}

.right{
	background: url('../img/rect.png');
	background-repeat: repeat-x;
	height: 44px;
	padding-right: 10px;
}

.left, .right{
	display: table-cell;
}

.divTable{
	display: table;
	width:100%;
}
.row{
	display:table-row;
}

/*canvas{
	border:1px solid #fff;
}*/

#launch{
	margin-top: 10px;
	display: inline-block;
	text-decoration: none;
	border-radius: 6px 6px 6px 6px;
	font-family: "SapientSansLight";
    font-size: 16px;
    padding: 5px 10px;
}

.btn-primary.active,
.btn-warning.active,
.btn-danger.active,
.btn-success.active,
.btn-info.active,
.btn-inverse.active {
  color: rgba(255, 255, 255, 0.75);
}
.btn-primary {
  color: #ffffff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #006dcc;
  background-image: -moz-linear-gradient(top, #0088cc, #0044cc);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#0088cc), to(#0044cc));
  background-image: -webkit-linear-gradient(top, #0088cc, #0044cc);
  background-image: -o-linear-gradient(top, #0088cc, #0044cc);
  background-image: linear-gradient(to bottom, #0088cc, #0044cc);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff0088cc', endColorstr='#ff0044cc', GradientType=0);
  border-color: #0044cc #0044cc #002a80;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  *background-color: #0044cc;
  /* Darken IE7 buttons by default so they stand out more given they won't have borders */

  filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
}
.btn-primary:hover,
.btn-primary:focus,
.btn-primary:active,
.btn-primary.active,
.btn-primary.disabled,
.btn-primary[disabled] {
  color: #ffffff;
  background-color: #0044cc;
  *background-color: #003bb3;
}
.btn-primary:active,
.btn-primary.active {
  background-color: #003399 \9;
}